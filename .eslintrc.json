{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:@shopify/typescript",
        "plugin:@shopify/react",
        "plugin:@shopify/prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": 2,
      "@shopify/jsx-no-hardcoded-content": 0
    }
}
